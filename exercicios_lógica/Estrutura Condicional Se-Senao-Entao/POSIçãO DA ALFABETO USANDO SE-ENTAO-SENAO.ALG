                                       escreval ("A letra ",letra," esta na posição",12 ," do alfabeto")
                                    senao
                                       se (letra = "m") Ou (letra = "M")  entao
                                          escreval ("A letra ",letra," esta na posição",13 ," do alfabeto")
                                       senao
                                          se (letra = "n") Ou (letra = "N") entao
                                             escreval ("A letra ",letra," esta na posição",14 ," do alfabeto")
                                          senao
                                             se (letra = "o") Ou (letra = "O") entao
                                                escreval ("A letra ",letra," esta na posição",15 ," do alfabeto")
                                             senao
                                                se (letra = "p") Ou (letra = "P") entao
                                                   escreval ("A letra ",letra," esta na posição",16 ," do alfabeto")
                                                senao
                                                   se (letra = "q") Ou (letra = "Q") entao
                                                      escreval ("A letra ",letra," esta na posição",17 ," do alfabeto")
                                                   senao
                                                      se (letra = "r") Ou (letra = "R") entao
                                                         escreval ("A letra ",letra," esta na posição",18 ," do alfabeto")
                                                      senao
                                                         se (letra = "s") Ou (letra = "S") entao
                                                            escreval ("A letra ",letra," esta na posição",19 ," do alfabeto")
                                                         senao
                                                            se (letra = "t") Ou (letra = "T") entao
                                                               escreval ("A letra ",letra," esta na posição",20 ," do alfabeto")
                                                            senao
                                                               se (letra = "u") Ou (letra = "U") entao
                                                                  escreval ("A letra ",letra," esta na posição",21 ," do alfabeto")
                                                               senao
                                                                  se (letra = "v") Ou (letra = "V") entao
