Algoritmo "posição no alfabeto usando se-entao-senao"
// Disciplina   : [Lógica de Programação]
Var
   letra : caracter

Inicio
   Escreval ("----- Descubra a posição de uma letra no alfabeto! -----")
   Escreva ("Digite a letra: ")
   leia(letra)
   se (letra = "a") Ou (letra = "A") entao
      escreva("A letra ",letra," esta na posição",1 ," do alfabeto")
   senao
      se (letra = "b") Ou (letra = "B")  entao
         escreva("A letra ",letra," esta na posição",2 ," do alfabeto")
      senao
         se (letra = "c") Ou (letra = "C")  entao
            escreva("A letra ",letra," está na posição",3 ," do alfabeto")
         senao
            se (letra = "d") Ou (letra = "D") entao
               escreva("A letra ",letra," está na posição",4 ," do alfabeto")
            senao
               se (letra = "e") Ou (letra = "E") entao
                  escreva("A letra ",letra," esta na posição",5 ," do alfabeto")
               senao
                  se (letra = "f") Ou (letra = "F")  entao
                     escreva("A letra ",letra," esta na posição",6 ," do alfabeto")
                  senao
                     se (letra = "g") Ou (letra = "G")  entao
                        escreval("A letra ",letra," esta na posição",7 ," do alfabeto")
                     senao
                        se (letra = "h") Ou (letra = "H")  entao
                           escreval ("A letra ",letra," esta na posição",8 ," do alfabeto")
                        senao
                           se (letra = "i") Ou (letra = "I")  entao
                              escreval ("A letra ",letra," esta na posição",9 ," do alfabeto")
                           senao
