                                                                  senao
                                                                     se (letra = "x") Ou (letra = "X") entao
                                                                        escreval ("A letra ",letra," esta na posição",23 ," do alfabeto")
                                                                     senao
                                                                        se (letra = "w") Ou (letra = "W") entao
                                                                           escreval ("A letra ",letra," esta na posição",24 ," do alfabeto")
                                                                        senao
                                                                           se (letra = "y") Ou (letra = "Y") entao
                                                                              escreval ("A letra ",letra," esta na posição",25 ," do alfabeto")
                                                                           senao
                                                                              se (letra = "z") Ou (letra = "Z") entao
                                                                                 escreval ("A letra ",letra," esta na posição",26 ," do alfabeto")
